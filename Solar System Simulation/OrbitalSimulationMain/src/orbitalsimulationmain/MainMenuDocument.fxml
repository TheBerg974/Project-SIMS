<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="orbitalsimulationmain.MainMenuDocumentController">
   <children>
      <Label fx:id="labelMainMenu" layoutX="212.0" layoutY="90.0" text="Choose what scenario you prefer:" />
      <Button fx:id="buttonBlankScenario" layoutX="81.0" layoutY="209.0" mnemonicParsing="false" onAction="#handleButtonBlankScenarioAction" text="Blank scenario" />
      <Button fx:id="buttonEarthMoon" layoutX="228.0" layoutY="209.0" mnemonicParsing="false" onAction="#handleButtonEarthMoonAction" text="Earth-Moon simulation" />
      <Button fx:id="buttonEarthSun" layoutX="429.0" layoutY="209.0" mnemonicParsing="false" onAction="#handleButtonEarthSunAction" text="Earth-Sun simulation" />
      <Button fx:id="buttonMichaelBayMode" layoutX="175.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleButtonMichaelBayModeAction" prefHeight="30.0" prefWidth="250.0" text="Enable Michael Bay Mode" />
      <ImageView fitHeight="150.0" fitWidth="150.0" layoutX="30.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="150.0" fitWidth="150.0" layoutX="420.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
